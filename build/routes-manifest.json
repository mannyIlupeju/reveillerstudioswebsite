npm install -g vercel{
  "version": 3,
  "pages404": true,
  "caseSensitive": false,
  "basePath": "",
  "redirects": [
    {
      "source": "/:path+/",
      "destination": "/:path+",
      "internal": true,
      "statusCode": 308,
      "regex": "^(?:/((?:[^/]+?)(?:/(?:[^/]+?))*))/$"
    }
  ],
  "headers": [],
  "dynamicRoutes": [
    {
      "page": "/shop/allProducts/[slug]",
      "regex": "^/shop/allProducts/([^/]+?)(?:/)?$",
      "routeKeys": {
        "nxtPslug": "nxtPslug"
      },
      "namedRegex": "^/shop/allProducts/(?<nxtPslug>[^/]+?)(?:/)?$"
    },
    {
      "page": "/shop/collections/[slug]",
      "regex": "^/shop/collections/([^/]+?)(?:/)?$",
